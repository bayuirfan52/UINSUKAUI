<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.UINSUKAUI.TextView.Bold.Black.Large" parent="Widget.AppCompat.TextView">
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="Theme.UINSUKAUI.TextView.Regular.Black.Medium" parent="Widget.AppCompat.TextView">
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="Theme.UINSUKAUI.TextView.Regular.Black" parent="Widget.AppCompat.TextView">
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="Theme.UINSUKAUI.Button.Black" parent="Widget.AppCompat.Button.Colored">
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:drawablePadding">15dp</item>
        <item name="android:background">@drawable/state_button_black</item>
    </style>

    <style name="Theme.UINSUKAUI.EditText.Outline" parent="Widget.AppCompat.EditText">
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/bg_edittext_outline</item>
    </style>
</resources>